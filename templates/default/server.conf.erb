input {
  <% if @inputs.empty? -%>
  <% else -%> 
    <%= LogstashConf.section_to_str(@inputs) %>
  <% end -%>
}

<% unless @filters.empty? -%>
filter {
  <%= LogstashConf.section_to_str(@filters, node['logstash']['server']['version'], @patterns_dir) %>
}
<% end -%>

output {
  <% if @output.empty? -%>
    <% else -%>
      <%= LogstashConf.section_to_str(@output) %>
  <% end -%>
}
